{
    "uidCookie": {
        "salt": "",
        "name": "_dsuid"
    },
    "assetsPath": "/home/newsnow/dockside/app/server/assets",
    "reservationsPath": "/data/db/reservations.json",
    "containersPath": "/data/cache/containers.json",
    "tmpPath": "/tmp/dockside",
    "globalCookie": {
        "name": "",
        "secret": "",
        "domain": ""
    },
    "containerCookie": {
        "name": "CAUTH",
        "domain": ""
    },
    "gateway": {
      "IP": "172.17.0.1",
      "enabled": false
    },
    "ide": {
        "path": "/opt/dockside",
        "env": {
            "SSHD_HOSTKEYS": "/etc/dropbear"
         },
         "command": [
            "/opt/dockside/launch.sh",
            "IDE_USER='{ideUser}'",
            "GIT_COMMITTER_NAME='{user.name}'",
            "GIT_COMMITTER_EMAIL='{user.email}'",
            "launch_ide"
        ]
    },
    "ssh": {
      "default": true,
      "sharedHostKeys": {
         "enabled": true,
         "mountpoint": "/etc/dropbear",
         "volume": "dockside-sshd-hostkeys"
      }
    },
    "clientDistPath": "/home/newsnow/dockside/app/client/dist",
    "docker": {
        "bin": "/usr/bin/docker",
        "socket": "/var/run/docker.sock",
        "PATH": "/usr/bin:/bin",
        "HOME": "/home/newsnow",
        "security": {
            "apparmor": "unspecified",
            "seccomp": "unspecified"
        }
    },
    "lxcfs": {
        "mountpoints": [
            "/proc/cpuinfo",
            "/proc/diskstats",
            "/proc/meminfo",
            "/proc/stat",
            "/proc/swaps",
            "/proc/uptime",
            "/proc/loadavg",
            "/sys/devices/system/cpu/online"
        ],
        "mountpoint": "/var/lib/lxcfs",
        "available": false,
        "default": true
    }
}
